  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@types/node": "^20.14.10",
    "allure-commandline": "^2.29.0",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "allure-playwright": "^2.15.1",
    "dotenv": "^16.4.5",
    "winston": "^3.13.1"
  }
}
